  <body>

      <span class="data" style="display:none"><%= data %></span>

      <h3 align="center"> TWEET COUNT ACCORDING TO GENDER </h3>
      <div id="piechart_3d" style="width:900px; height: 500px; margin:0 auto; background-color: #FFF;"></div>
      <!-- <div id="chart_div" style="width:900px; height: 1500px; margin:0 auto;"></div> -->


      <div class="datagrid">
          <h3 align="center"> FAITHFUL SUPPORTERS </h3>
          <table>
              <thead><tr><th></th><th>Male</th><th>Female</th><th>Brand</th></tr></thead>
              <tbody><tr class="alt"><td>Faithful supporters</td><td><%= rawdata.objGenderMale.count %> tweets</td><td><%= rawdata.objGenderFemale.count %> tweets</td><td><%= rawdata.objGenderBrand.count %> tweets</td></tr>
                  <tr><td></td><td><%= rawdata.objGenderMale.user %></td><td><%= rawdata.objGenderFemale.user %></td><td><%= rawdata.objGenderBrand.user %></td></tr>
              </tbody>
          </table>
      </div>


